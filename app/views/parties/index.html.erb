<h1>Listing parties</h1>

<table class="table">
  <tr>
    <th>Title</th>
    <th>Actions</th>
  </tr>

  <% @parties.each do |party| %>
    <tr>
      <td><%= party.title %></td>
      <td>
        <%= link_to 'Show', edit_party_path(party), class: "btn btn-info" %>
        <%= link_to 'Destroy', party, method: :delete, data: { confirm: 'Are you sure?'}, class: "btn btn-danger" %>
      </td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New Party', new_party_path, class: "btn btn-default" %>
